{
    "name": "hojjabr/persianslug",
    "description": "Persian Slug generator for Laravel 6",
    "license": "The Unlicense",
    "authors": [
        {
            "name": "Hossein Jabbari",
            "email": "mr.hhkj@gmail.com",
            "homepage": "https://github.com/hojjabr"
        }
    ],
    "homepage": "https://github.com/hojjabr/persianslug",
    "keywords": ["Laravel", "PersianSlug"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "hojjabr\\PersianSlug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hojjabr\\PersianSlug\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hojjabr\\PersianSlug\\PersianSlugServiceProvider"
            ],
            "aliases": {
                "PersianSlug": "hojjabr\\PersianSlug\\Facades\\PersianSlug"
            }
        }
    }
}
